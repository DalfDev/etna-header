{
	"name": "etna-header",
	"displayName": "ETNA-HEADER",
	"description": "Add etna header's to your files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.etna-header"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.etna-header",
				"title": "Add Etna Header"
			}
        ],
        "keybindings":[
            {
                "command": "extension.etna-header",
                "key": "ctrl+c h"
            }
        ]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"vscode": "^1.1.36"
	}
}
